<Window x:Class="CalculadoraWpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CalculadoraWpf"
        xmlns:y="clr-namespace:CalculadoraWpf.Views"
        
        mc:Ignorable="d"
        Title="Tela Inicial" Height="450" Width="800">
    <Grid>

        <y:ucLogin x:Name="ucLoginForm" loginCorrect="UcLoginForm_loginCorrect"></y:ucLogin>

        <y:ucTaskBar x:Name="ucTaskForm" Visibility="Hidden"  superatomica="UcTaskForm_superatomica"></y:ucTaskBar>
        <y:ucSuperCal x:Name="ucSuperForm" Visibility="Hidden"></y:ucSuperCal>
        
    </Grid>
</Window>
